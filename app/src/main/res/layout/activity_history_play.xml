<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_history_play"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.atlas.cscc20.Activity.HistoryPlayActivity">
    <LinearLayout
        android:layout_width="1920px"
        android:layout_height="1080px"
        android:layout_centerInParent="true"
        android:background="@drawable/history_bg">

        <LinearLayout
            android:orientation="vertical"
            android:layout_marginLeft="253px"
            android:layout_marginTop="200px"
            android:layout_width="1247px"
            android:layout_height="658px"
            android:id="@+id/history_view">
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/iv_ret"
        android:layout_width="130dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="70dp"
        android:src="@drawable/ret" />
</RelativeLayout>
